{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "generateMigrations": "drizzle-kit generate:sqlite",
    "updateDB": "drizzle-kit push:sqlite; sqlite3 ./sqlite.db < ./drizzle/0004_empty_killer_shrike.sql",
    "updateDBWindows": "drizzle-kit push:sqlite && C:\\sqlite\\sqlite3.exe .\\sqlite.db < ./drizzle/0004_empty_killer_shrike.sql",
    "showDB": "drizzle-kit studio",
    "seed": "npx tsx -e \"require('./seed/seed.ts').seed(require('./seed/seedData.ts').seedData, true);\"",
    "updateTestDBWindows": "set database_url=test/sqlite.db&&drizzle-kit push:sqlite && C:/sqlite/sqlite3.exe .\\test\\sqlite.db < ./drizzle/0004_empty_killer_shrike.sql",
    "showTestDBWindows": "set DATABASE_URL=test/sqlite.db && drizzle-kit studio",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/test-utils": "^3.7.4",
    "@sidebase/nuxt-auth": "^0.6.0-beta.7",
    "@types/better-sqlite3": "^7.6.5",
    "drizzle-kit": "^0.19.13",
    "nuxt": "^3.7.4",
    "tsx": "^3.14.0",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.3",
    "@nuxtjs/color-mode": "^3.3.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sidebase/nuxt-auth": "^0.6.0-beta.7",
    "auth": "^0.0.9",
    "better-sqlite3": "^8.7.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "next-auth": "^4.21.1",
    "ngrok": "^5.0.0-beta.2"
  }
}